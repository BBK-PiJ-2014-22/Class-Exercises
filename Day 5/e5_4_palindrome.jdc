while (true){
	println(Palindrome(System.console().readLine()));
}

boolean Palindrome(String word){
	if (word.length() <= 2){
		if (word.length() == 1 ||
		    word.charAt(0) == word.charAt(1)){
			return true;
		}else{
			return false;
		}
	}else if (Palindrome(word.substring(1,word.length()-1))){
		return true;
	}else {
		return false;
	}
}